<div class="page">
  <header role="banner">
    {{ page.header }}
  </header>
  {{ page.primary_menu }}
  {{ page.highlighted }}
  <main role="main">
    <a id="main-content" tabindex="-1"></a>
    {{ page.content}}
  </main>
  {% if page.footer %}
  <footer role="contentinfo">
    {{ page.footer }}
  </footer>
  {% endif %}
</div>
